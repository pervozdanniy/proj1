{
  "name": "skopa",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "node_modules/.bin/nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "node_modules/.bin/nest start $npm_config_component",
    "start:dev": "node_modules/.bin/nest start --watch",
    "start:debug": "node_modules/.bin/nest start --debug --watch",
    "start:prod": "node dist/apps/$npm_config_component/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./jest-e2e.json --runInBand",
    "prepare": "husky install",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli",
    "typeorm:run": "cross-var npm run typeorm -- -d ./apps/$npm_config_component/src/db/data-source.ts migration:run",
    "typeorm:generate": "cross-var npm run typeorm -- -d ./apps/$npm_config_component/src/db/data-source.ts migration:generate ./apps/$npm_config_component/src/db/migrations/$npm_config_name",
    "typeorm:create": "cross-var npm run typeorm -- migration:create ./apps/$npm_config_component/src/db/migrations/$npm_config_name",
    "typeorm:revert": "cross-var npm run typeorm -- -d ./apps/$npm_config_component/src/db/data-source.ts migration:revert",
    "proto:generate": "node_modules/.bin/protoc --plugin=node_modules/.bin/protoc-gen-ts_proto.cmd --ts_proto_opt=nestJs=true --ts_proto_opt=addGrpcMetadata=false --ts_proto_opt=snakeToCamel=false --ts_proto_opt=addNestjsRestParameter=true --ts_proto_out=./common/grpc/interfaces --proto_path=./common/grpc/_proto ./common/grpc/_proto/*.proto"
  },
  "dependencies": {
    "@aws-sdk/client-kms": "~3.272.0",
    "@aws-sdk/client-s3": "~3.272.0",
    "@aws-sdk/client-sqs": "~3.272.0",
    "@grpc/grpc-js": "^1.7.3",
    "@grpc/proto-loader": "^0.7.3",
    "@liaoliaots/nestjs-redis": "^9.0.4",
    "@nestjs/axios": "^1.0.0",
    "@nestjs/bull": "^0.6.2",
    "@nestjs/common": "~9.2.1",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "~9.2.1",
    "@nestjs/jwt": "~9.0.0",
    "@nestjs/microservices": "~9.2.1",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "~9.2.1",
    "@nestjs/platform-socket.io": "~9.2.1",
    "@nestjs/schedule": "^2.2.2",
    "@nestjs/swagger": "^6.1.3",
    "@nestjs/terminus": "~9.2.1",
    "@nestjs/typeorm": "~9.0.1",
    "@nestjs/websockets": "~9.2.1",
    "@sendgrid/mail": "^7.7.0",
    "@sentry/node": "^7.23.0",
    "axios": "~1.1.3",
    "bcrypt": "^5.1.0",
    "bull": "^4.10.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "fraction.js": "^4.2.0",
    "cookie-parser": "^1.4.6",
    "hbs": "^4.2.0",
    "ioredis": "^5.2.4",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "pg": "^8.8.0",
    "pg-cursor": "^2.10.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "socket.io": "^4.5.4",
    "typeorm": "^0.3.10",
    "uid-safe": "^2.1.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/bull": "^4.10.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cron": "^2.0.1",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.8",
    "@types/lodash": "^4.14.194",
    "@types/ms": "^0.7.31",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.11.0",
    "@types/passport-jwt": "^3.0.7",
    "@types/pg-cursor": "^2.7.0",
    "@types/supertest": "^2.0.11",
    "@types/uid-safe": "^2.1.2",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.0.0",
    "cross-var": "^1.1.0",
    "eslint": "8.22.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.2",
    "jest": "28.1.3",
    "jest-resolver-tsconfig-paths": "^0.14.4",
    "prettier": "^2.3.2",
    "prettier-plugin-organize-imports": "^3.2.1",
    "protoc": "^1.1.3",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.0.0",
    "ts-proto": "^1.135.0",
    "tsconfig-paths": "^4.1.1",
    "typescript": "^4.9.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/"
    ]
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.7",
    "utf-8-validate": "^6.0.1"
  }
}
