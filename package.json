{
  "name": "skopa",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "node_modules/.bin/nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "node_modules/.bin/nest start",
    "start:dev": "node_modules/.bin/nest start --watch",
    "start:debug": "node_modules/.bin/nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/core/test/jest-e2e.json",
    "prepare": "husky install",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli",
    "typeorm:run": "npm run typeorm -- -d ./apps/$npm_config_component/src/db/data-source.ts migration:run",
    "typeorm:generate": "npm run typeorm -- -d ./apps/$npm_config_component/src/db/data-source.ts migration:generate ./apps/$npm_config_component/src/db/migrations/$npm_config_name",
    "typeorm:create": "npm run typeorm -- migration:create ./apps/$npm_config_component/src/db/migrations/$npm_config_name",
    "typeorm:revert": "npm run typeorm -- -d ./apps/$npm_config_component/src/db/data-source.ts migration:revert",
    "proto:generate": "node_modules/.bin/protoc --plugin=node_modules/.bin/protoc-gen-ts_proto.cmd --ts_proto_opt=nestJs=true --ts_proto_opt=addGrpcMetadata=true --ts_proto_opt=snakeToCamel=false --ts_proto_out=./common/grpc/interfaces --proto_path=./common/grpc/_proto  ./common/grpc/_proto/*.proto"
  },
  "dependencies": {
    "@aws-sdk/client-sqs": "^3.224.0",
    "@grpc/grpc-js": "^1.7.3",
    "@grpc/proto-loader": "^0.7.3",
    "@liaoliaots/nestjs-redis": "^9.0.4",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/microservices": "^9.2.1",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^6.1.3",
    "@nestjs/typeorm": "^9.0.1",
    "@types/uid-safe": "^2.1.2",
    "aws-sdk": "^2.1269.0",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "ioredis": "^5.2.4",
    "jsonwebtoken": "^8.5.1",
    "nest-aws-sdk": "^3.0.0",
    "nestjs-pino": "^3.1.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.8.0",
    "pino-http": "^8.2.1",
    "pino-pretty": "^9.1.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.10",
    "uid-safe": "^2.1.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@ntegral/nestjs-sentry": "^4.0.0",
    "@sentry/hub": "^7.23.0",
    "@sentry/minimal": "^6.19.7",
    "@sentry/node": "^7.23.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.8",
    "@types/node": "^18.11.0",
    "@types/passport-jwt": "^3.0.7",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "8.22.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.2",
    "jest": "28.1.3",
    "prettier": "^2.3.2",
    "protoc": "^1.1.3",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.0.0",
    "ts-proto": "^1.135.0",
    "tsconfig-paths": "^4.1.1",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/"
    ]
  }
}
